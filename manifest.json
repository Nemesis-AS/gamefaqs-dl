{
  "manifest_version": 3,
  "name": "GameFAQs DL",
  "version": "1.0",
  "description": "An extension to download GameFAQs guides",
  "content_scripts": [
    {
      "js": ["scripts/dl.js"],
      "matches": [
        "https://gamefaqs.gamespot.com/*"
      ]
    }
  ],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
